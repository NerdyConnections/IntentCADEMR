@using IntentCADEMR.Util
@using  System.Configuration;
@model IntentCADEMR.Models.RegistrationInfoModel
@{
    ViewBag.Title = "Register";
    Layout = "~/Views/Shared/_LoginMaster.cshtml";
}

<style>
    .errorMssg {
        color: red;
    }
</style>


@using (Html.BeginForm("Register", "Account", FormMethod.Post, new { @id = "frmRegistration" }))
{

    <div class="row" style="padding-bottom:100px; padding-top: 75px; font-size:24px; background-color:#eeeeee">

        <div class="center-block">
            <div class="row">
                <div class="col-sm-12">
                    <img class="img-responsive" src="/img/login_banner_img.png" width="600" height="150">
                </div>

                <div class="col-sm-12" style="background-color:#D9E1E3; text-align:center; padding:10px 0px 10px 0px">
                    <h1 class="blue">REGISTRATION FORM</h1>
                    <h5>
                        Please complete all the required fields below and create your username (your email address) and password.<br>
                        * REQUIRED FIELDS
                    </h5>
                </div>

                <div class="col-sm-12" style="background-color:#D9E1E3; padding:0px 50px 0px 50px; text-align:center; color:#005b7f">
                    Contact Information
                </div>
                <div class="col-sm-12" style="background-color:#D9E1E3; padding:20px 50px 0px 50px">
                    <label class="control-label">First Name:* </label>
                    @Html.TextBoxFor(model => model.FirstName, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.FirstName, "", new { @class = "text-danger" })
                </div>

                <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px">
                    <label class="control-label">Last Name:*</label>
                    @Html.TextBoxFor(model => model.LastName, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.LastName, "", new { @class = "text-danger" })
                </div>
                <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px">
                    <label class="control-label">Clinic Name:* </label>
                    @Html.TextBoxFor(model => model.ClinicName, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.ClinicName, "", new { @class = "text-danger" })
                </div>
                <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px">
                    <label class="control-label">Mailing Address:* </label>
                    @Html.TextBoxFor(model => model.MailingAddress, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.MailingAddress, "", new { @class = "text-danger" })
                </div>

                <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px">
                    <label class="control-label">City:* </label>
                    @Html.TextBoxFor(model => model.City, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.City, "", new { @class = "text-danger" })
                </div>

                <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px">
                    <label class="control-label">
                        Province: please select one from the drop down menu
                    </label>
                    <div class="col-sm-12">
                        @Html.DropDownListFor(model => model.Province, new SelectList(UserHelper.GetProvinces(), "Value", "Text"), "Please select a province", new { @class = "form-control" })
                        @Html.ValidationMessageFor(m => m.Province, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px">
                    <label class="control-label">Postal Code:* </label>
                    @Html.TextBoxFor(model => model.PostalCode, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.PostalCode, "", new { @class = "text-danger" })
                </div>

                <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px">
                    <label class="control-label">Phone Number:* </label>
                    @Html.TextBoxFor(model => model.PhoneNumber, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.PhoneNumber, "", new { @class = "text-danger" })


                </div>
                <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px">
                    <label class="control-label">Fax Number:* </label>
                    @Html.TextBoxFor(model => model.FaxNumber, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.FaxNumber, "", new { @class = "text-danger" })
                </div>

                <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px; text-align:center; color:#005b7f">
                    Demographics and Practice Information:
                </div>
                <div class="col-sm-12" style="background-color:#D9E1E3; padding:20px 50px 0px 50px">
                    <label class="control-label"><strong>Gender:* </strong></label>
                    <div>
                        <label>
                            @Html.RadioButtonFor(model => model.Gender, "Male")
                            &nbsp;&nbsp;Male
                        </label>
                    </div>
                    <div>
                        <label>
                            @Html.RadioButtonFor(model => model.Gender, "Female")
                            &nbsp;&nbsp;Female
                        </label>
                    </div>
                    <div>
                        <label>
                            @Html.RadioButtonFor(model => model.Gender, "Prefer not to state")
                            &nbsp;&nbsp;Prefer not to state
                        </label>
                    </div>
                    @Html.ValidationMessageFor(model => model.Gender, "", new { @class = "text-danger" })
                </div>

                <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px">
                    <label class="control-label">
                        <strong>Medical Specialty*</strong><br>
                        (Please select one. If other, please specify):
                    </label>


                    <div>
                        <label>
                            @Html.RadioButtonFor(model => model.MedicalSpecialty, "Primary Care Physician", new { @class = "MedicalSpecialty" })
                            &nbsp;&nbsp;Primary Care Physician
                        </label>
                    </div>
                    <div>
                        <label>
                            @Html.RadioButtonFor(model => model.MedicalSpecialty, "Cardiologist", new { @class = "MedicalSpecialty" })
                            &nbsp;&nbsp;Cardiologist
                        </label>
                    </div>
                    <div>
                        <label>
                            @Html.RadioButtonFor(model => model.MedicalSpecialty, "Internal Medicine Specialist", new { @class = "MedicalSpecialty" })
                            &nbsp;&nbsp;Internal Medicine Specialist
                        </label>
                    </div>
                    <div>
                        <label>
                            @Html.RadioButtonFor(model => model.MedicalSpecialty, "Endocrinologist", new { @class = "MedicalSpecialty" })
                            &nbsp;&nbsp;Endocrinologist
                        </label>
                    </div>


                    <div class="col-sm-12" style="margin-top:0px; padding-top:0px; padding-left:0px">
                        @Html.RadioButtonFor(model => model.MedicalSpecialty, "Other", new { @id = "MedicalSpecialty", @class = "MedicalSpecialty" })
                        <label class="" title="Other Medical Specialty">&nbsp;&nbsp;Other, Specify:</label><br />
                        @Html.ValidationMessageFor(model => model.MedicalSpecialty, "", new { @class = "text-danger" })
                        @Html.TextBoxFor(model => model.OtherMedicalSpecialty, new { @class = "form-control", @id = "OtherMedicalSpecialty" })
                        <label id="OtherMedicalSpecialtyMsg" class="text-danger"></label>
                    </div>
                </div>

                <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px">
                    <label class="control-label">
                        <strong>Which of the following best describes your practice setting?*</strong><br>
                        (Please select one. If other, please specify):
                    </label>
              
                <div>
                    <label>
                        @Html.RadioButtonFor(model => model.PracticeSetting, "Solo Practice")
                        &nbsp;&nbsp;Solo Practice
                    </label>
                </div>
                <div>
                    <label>
                        @Html.RadioButtonFor(model => model.PracticeSetting, "Group Practice")
                        &nbsp;&nbsp;Group Practice
                    </label>
                </div>
                <div>
                    <label>
                        @Html.RadioButtonFor(model => model.PracticeSetting, "Family Health Team")
                        &nbsp;&nbsp;Family Health Team
                    </label>
                </div>
                <div>
                    <label>
                        @Html.RadioButtonFor(model => model.PracticeSetting, "Community Health Centre")
                        &nbsp;&nbsp;Community Health Centre
                    </label>
                </div>
                <div>
                    <label>
                        @Html.RadioButtonFor(model => model.PracticeSetting, "Family Health Organization")
                        &nbsp;&nbsp;Family Health Organization
                    </label>
                </div>

                <div class="col-sm-12" style="margin-top:0px; padding:0px">
                    @Html.RadioButtonFor(model => model.PracticeSetting, "Other")
