                    @Html.ValidationMessageFor(model => model.PracticeSetting, "", new { @class = "text-danger" })
                    @Html.TextBoxFor(model => model.OtherPracticeSetting, new { @class = "form-control", @id = "OtherPracticeSetting" })
                    <!--label id="OtherPracticeSettingMsg" class="text-danger"></!--label-->
                </div>
            </div>

            <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px">
                <label class="control-label"><strong>How many years have you been in practice? *:</strong></label>
                <div>
                    <label>
                        @Html.RadioButtonFor(model => model.PracticeYears, "< 5 years")
                        &nbsp;&nbsp;&lt; 5 years
                    </label>
                </div>
                <div>
                    <label>
                        @Html.RadioButtonFor(model => model.PracticeYears, "5 – 10 years")
                        &nbsp;&nbsp;5 – 10 years
                    </label>
                </div>
                <div>
                    <label>
                        @Html.RadioButtonFor(model => model.PracticeYears, "11-15 years")
                        &nbsp;&nbsp;11-15 years
                    </label>
                </div>
                <div>
                    <label>
                        @Html.RadioButtonFor(model => model.PracticeYears, "16-20 years")
                        &nbsp;&nbsp;16-20 years
                    </label>
                </div>
                <div>
                    <label>
                        @Html.RadioButtonFor(model => model.PracticeYears, "21 – 25 years")
                        &nbsp;&nbsp;21 – 25 years
                    </label>
                </div>
                <div>
                    <label>
                        @Html.RadioButtonFor(model => model.PracticeYears, "> 25 years")
                        &nbsp;&nbsp;&gt; 25 years
                    </label><br />
                    @Html.ValidationMessageFor(model => model.PracticeYears, "", new { @class = "text-danger" })
                </div>
            </div>
        </div>

