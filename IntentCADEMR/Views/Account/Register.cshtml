        <div class="col-sm-12" style="background-color:#D9E1E3; padding:30px 50px 0px 50px">
            <label class="control-label"><strong>How familiar are you with the results of the COMPASS Trial? </strong>(Select ONE) </label>
            <br />
            <div>
                <label>
                    @Html.RadioButtonFor(model => model.FamiliarCOMPASSTrial, "Not at all familiar")
                    &nbsp;&nbsp;Not at all familiar
                </label>
            </div>
            <div>
                <label>
                    @Html.RadioButtonFor(model => model.FamiliarCOMPASSTrial, "Somewhat familiar")
                    &nbsp;&nbsp;Somewhat familiar
                </label>
            </div>
            <div>
                <label>
                    @Html.RadioButtonFor(model => model.FamiliarCOMPASSTrial, "Very familiar")
