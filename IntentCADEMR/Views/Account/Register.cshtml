                    </tr>
                    <tr>
                        <td style="padding-left:10px">
                            <h5>Diabetes Mellitus</h5>
                            @Html.ValidationMessageFor(model => model.ProportionDM, "", new { @class = "text-danger" })
                        </td>
                        <td align="center">
                            @Html.RadioButtonFor(model => model.ProportionDM, "< 25%")

                        </td>
                        <td align="center">
                            <label>
                                @Html.RadioButtonFor(model => model.ProportionDM, "25-50%")
                            </label>
                        </td>
                        <td align="center">
                            <label>
                                @Html.RadioButtonFor(model => model.ProportionDM, "51-75%")
                            </label>
                        </td>
                        <td align="center">
                            <label>
                                @Html.RadioButtonFor(model => model.ProportionDM, "> 75%")
                            </label>
                        </td>
                        <td align="center">
                            <label>
                                @Html.RadioButtonFor(model => model.ProportionDM, "Not Sure")
                            </label>
                        </td>
                    </tr>
                    <tr>
                        <td style="padding-left:10px">
                            <h5>Renal Dysfunction</h5>
                            @Html.ValidationMessageFor(model => model.ProportionRD, "", new { @class = "text-danger" })
