<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="successLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="successful_modal">
                    <p><strong>REGISTRATION SUCCESSFUL</strong></p>
                    <p>
                                                                       You have successfully completed your profile and are now registered for the INTENT-CAD program. A Registration Confirmation email has been sent to your email address. 
                    </p>
                    <p>

                        If you do not see the confirmation email in your inbox, please check your spam folder. We recommend that you add webmaster@@chrc.net to your address book.
                    </p>
                </div>

                <div id="failed_modal" style="color:red">
                    This email has been registered.
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal">OK</button>
            </div>
        </div>

    </div>
</div>

<script>
    $('#btnSubmit').click(function () {
        console.log('submit frmRegistration');
        $('#frmRegistration').submit();
    });
    $(document).ready(function () {
        var isHttpPost = "@ViewBag.IsHttpPost".toLowerCase();
        var isSuccessful = "@ViewBag.IsSuccessful".toLowerCase();
        if (isHttpPost == "true") {
            if (isSuccessful == "true") {
                $("#successful_modal").show();
                $("#failed_modal").hide();
            } else {
                $("#successful_modal").hide();
                $("#failed_modal").show();
            }
            $("#myModal").modal('show');
        }

        $('#myModal').on('hidden.bs.modal', function () {
            if (isSuccessful == "true") {
                window.location = "/Account/Login";
            }
        })


        $(".MedicalSpecialty").change(function () {
            validateOtherMedicalSpecialty();
        });
        $("#formSubmitted").submit(function (e) {
            validateOtherMedicalSpecialty(e);
            validateOtherPracticeSetting(e);
            if ($('.barriers:checked').length == 0) {
                $("#barriersMsg").html("Required");
                e.preventDefault();
            }
        });

        $('input[name="PracticeSetting"]').change(function () {
            validateOtherPracticeSetting();
        });

        $('.barriers').change(function () {
            if ($(this).is("#NoneAbove")) {
                if ($(this).is(":checked")) {
                    $(".barriers:not(#NoneAbove)").prop("checked", false);
                }
            } else {
                if ($(this).is(":checked")) {
                    $("#NoneAbove").prop("checked", false);
                }
            }

            if ($('.barriers:checked').length == 0) {
                $("#barriersMsg").html("Required");
            } else {
                $("#barriersMsg").html("");
            }
        });



        function validateOtherMedicalSpecialty(e) {
            var value = $('input.MedicalSpecialty:checked').val();
            if (value == "Other") {
                if ($("#OtherMedicalSpecialty").val().length == 0) {
                    $("#OtherMedicalSpecialtyMsg").html("Required");
                    if (e != undefined) {
                        e.preventDefault();
                    }
                } else {
                    $("#OtherMedicalSpecialtyMsg").html("");
                }
            } else {
                $("#OtherMedicalSpecialtyMsg").html("");
                $("#OtherMedicalSpecialty").val("");
            }
        }

        function validateOtherPracticeSetting(e) {
            var value = $('input[name="PracticeSetting"]:checked').val();
            if (value == "Other") {
                if ($("#OtherPracticeSetting").val().length == 0) {
                    $("#OtherPracticeSettingMsg").html("Required");
                    if (e != undefined) {
                        e.preventDefault();
                    }
                } else {
                    $("#OtherPracticeSettingMsg").html("");
                }
            } else {
                $("#OtherPracticeSettingMsg").html("");
                $("#OtherPracticeSetting").val("");
            }
        }


    });
</script>