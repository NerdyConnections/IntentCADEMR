                            <label>
                                @Html.RadioButtonFor(model => model.ProportionPAD, "> 75%")
                            </label>
                        </td>
                        <td align="center">
                            <label>
                                @Html.RadioButtonFor(model => model.ProportionPAD, "Not Sure")
                            </label>
                        </td>
                    </tr>
                    <tr>
                        <td style="padding-left:10px">
                            <h5>Cerebrovascular Disease</h5>
                            @Html.ValidationMessageFor(model => model.ProportionCD, "", new { @class = "text-danger" })
                        </td>
                        <td align="center">
                            @Html.RadioButtonFor(model => model.ProportionCD, "< 25%")

                        </td>
                        <td align="center">
                            <label>
                                @Html.RadioButtonFor(model => model.ProportionCD, "25-50%")
                            </label>
                        </td>
                        <td align="center">
                            <label>
                                @Html.RadioButtonFor(model => model.ProportionCD, "51-75%")
